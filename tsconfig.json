{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/api/auth/[...nextauth]/route.js", "app/utils/config/mongodb.ts", "app/utils/models/AdminRegister.js", "app/admin/dashboard/page.jsx", "app/admin/(app/admin/users/page.jsxin/(overview)/lapp/admin/layout.jsx", "app/providers/SessionProvider.js", "app/ui/fonts.js", "app/ui/adminLoginForm.jsx", "app/ui/sidenav.jsx", "app/page.jsx", "app/layout.jsx"],
  "exclude": ["node_modules"]
}
